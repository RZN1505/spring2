#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/atob/2.1.2/node_modules/atob/bin/atob.js/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/atob/2.1.2/node_modules/atob/bin/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/atob/2.1.2/node_modules/atob/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/atob/2.1.2/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/atob/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules:/Users/ruarez/Desktop/node_modules:/Users/ruarez/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../atob/2.1.2/node_modules/atob/bin/atob.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/atob/2.1.2/node_modules/atob/bin/atob.js/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/atob/2.1.2/node_modules/atob/bin/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/atob/2.1.2/node_modules/atob/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/atob/2.1.2/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/atob/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules:/Users/ruarez/Desktop/node_modules:/Users/ruarez/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../../../../../../atob/2.1.2/node_modules/atob/bin/atob.js" "$@"
  ret=$?
fi
exit $ret
