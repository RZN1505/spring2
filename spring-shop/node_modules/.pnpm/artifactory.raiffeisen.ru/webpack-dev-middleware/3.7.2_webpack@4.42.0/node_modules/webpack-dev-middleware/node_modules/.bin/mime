#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/mime/2.4.6/node_modules/mime/cli.js/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/mime/2.4.6/node_modules/mime/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/mime/2.4.6/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/mime/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules:/Users/ruarez/Desktop/node_modules:/Users/ruarez/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../mime/2.4.6/node_modules/mime/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/mime/2.4.6/node_modules/mime/cli.js/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/mime/2.4.6/node_modules/mime/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/mime/2.4.6/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/mime/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules:/Users/ruarez/Desktop/node_modules:/Users/ruarez/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../../../../../../mime/2.4.6/node_modules/mime/cli.js" "$@"
  ret=$?
fi
exit $ret
