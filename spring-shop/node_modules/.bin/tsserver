#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/typescript/3.8.3/node_modules/typescript/bin/tsserver/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/typescript/3.8.3/node_modules/typescript/bin/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/typescript/3.8.3/node_modules/typescript/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/typescript/3.8.3/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/typescript/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules:/Users/ruarez/Desktop/node_modules:/Users/ruarez/node_modules:/Users/node_modules:/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/typescript/bin/tsserver/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/typescript/bin/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/typescript/node_modules" "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
  ret=$?
else 
  NODE_PATH="/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/typescript/3.8.3/node_modules/typescript/bin/tsserver/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/typescript/3.8.3/node_modules/typescript/bin/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/typescript/3.8.3/node_modules/typescript/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/typescript/3.8.3/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/typescript/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/artifactory.raiffeisen.ru/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/.pnpm/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules:/Users/ruarez/Desktop/node_modules:/Users/ruarez/node_modules:/Users/node_modules:/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/typescript/bin/tsserver/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/typescript/bin/node_modules:/Users/ruarez/Desktop/spring-shop/node_modules/typescript/node_modules" node  "$basedir/../typescript/bin/tsserver" "$@"
  ret=$?
fi
exit $ret
